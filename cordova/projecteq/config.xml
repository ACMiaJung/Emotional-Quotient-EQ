<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jandslab.eq" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Project EQ</name>
    <description>Project EQ: Keep balance in your life</description>
    <author email="james@jandslab.com" href="http://jandslab.com">
        J&amp;S Lab
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
	
	<platform name="android">
		<icon src="res/icon/android/ldpi.png" density="ldpi" />
		<icon src="res/icon/android/mdpi.png" density="mdpi" />
		<icon src="res/icon/android/hdpi.png" density="hdpi" />
		<icon src="res/icon/android/xhdpi.png" density="xhdpi" />
		<icon src="res/icon/android/xxhdpi.png" density="xxhdpi" />
		<icon src="res/icon/android/xxxhdpi.png" density="xxxhdpi" />

		<preference name="AndroidWindowSplashScreenAnimatedIcon" value="res/screen/android/image2vector.xml" />
		<preference name="orientation" value="portrait" />
	</platform>
</widget>
